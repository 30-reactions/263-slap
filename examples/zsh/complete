#!/usr/bin/env zsh

cargo r -q -- deps dirname pwd || exit 1

config="$(cd "$(dirname "${(%):-%N}")" && pwd)/../complete.yml"
eval "$(cargo r -q -- parse zsh _ -- "$@" <"$config")"
[[ -z "$_success" ]] && exit 1

printf '%s\n' \
"opt     = '$_opt_vals'
pos     = '$_pos_vals'
flag    = '$_flag_vals'
mode    = '$_mode_vals'
mvals   = '$_mvals_vals'
minvals = '$_minvals_vals'
maxvals = '$_maxvals_vals'

subcommand   -> '$_subcommand'
subcmd_scopt  = '$_subcmd_scopt_vals'
subcmd_scpos1 = '$_subcmd_scpos1_vals'"

